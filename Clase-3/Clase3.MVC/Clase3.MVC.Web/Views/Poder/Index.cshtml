@model List<Clase3.MVC.Dominio.Entidades.Poder>
@{
    ViewBag.Title = "Poderes";
}
<a href="/poder/Agregar">Agregar Poder</a>
<table>
    <tr>
        <th>
            Id
        </th>
        <th>
            Nombre
        </th>
        <th>
            Accion
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.ActionLink("Ver", "Detalle", "Poder", new { id=item.Id }) |
                @Html.ActionLink("Eliminar", "Eliminar", "Poder", new { id=item.Id }) |
                @Html.ActionLink("Editar", "FormularioEditarPoder", "Poder", new { id=item.Id })
            </td>
        </tr>
    }
</table>

